package hr.algebra.java;

public class Zadatak {
	private String username;
	private String password;
	

	public static void main(String[] args) {
		Zadatak z = new Zadatak();
		z.username = "Bob";
		z.password = "Rock";
		System.out.println("Output login info:" +"\n" + z.ispisSadrzaja());
		
	}


	private String ispisSadrzaja() {
		String dataI = "";
		dataI += "username: " + username + "\n";
		dataI += "password: " + password + "\n";
		return dataI;
	}




}
